}

/* wxss */
.checkbox {
  width: 80px;
  height: 32px;
  text-align: center;
  line-height: 32px;
  color: #333;
  border: 0.8px solid rgba(0, 0, 0, 0.5);
  border-radius: 26px;
  display: inline-block;
  position: relative
}

.checked {
  color: #F50137;
  background: rgba(245, 1, 55, 0.08);
  border: 1px solid #F50137;
}

.checkbox checkbox {
  display: none
}

.checked-img {
  width: 28rpx;
  height: 28rpx;
  position: absolute;
  top: 0;
  right: 0
}

.logo_img {
  width: calc(129rpx * 1.5);
  height: calc(60rpx * 1.5);
  margin: 0 auto;
}

.logo_img image {
  width: 100%;
  height: 100%;
}


.tips {
  position: fixed;
  left: 50%;
  top: 50%;
  margin-left: -100px;
  margin-top: -20px;
  width: 200px;
  height: 40px;
  text-align: center;
  line-height: 40px;
  color: #fff;
  border-radius: 26px;
  transition: 0.4s all;
  pointer-events: none;
  background-color: rgba(0, 0, 0, 0.8);
}

