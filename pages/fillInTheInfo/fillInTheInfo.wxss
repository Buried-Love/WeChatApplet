.iphone_pop_btn{
  width: 74.4%;
  height: 30px;
  padding: 0;
  text-align: center;
  font-size: 14px;
  line-height: 30px;
  border: none!important;
}

.btn {
  width: 90.4%;
  height: 40px;
  font-size: 16px;
  font-weight: bold;
  margin: 20px auto;
  border-radius: 26px;
  color: #fff;
  transition: 0.2s all;
  border: 0.5px solid #F50137;
  background-color: #F50137;
}

.btn-hover {
  border: 0.5px solid #F50137;
  background-color: #FF6184;
}

switch .wx-switch-input:not([class*="bg-"]) {
