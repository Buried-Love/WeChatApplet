  align-items: center;
  position: relative;
  text-shadow: #000 4rpx 0 0, #000 0 4rpx 0, #000 -4rpx 0 0, #000 0 -4rpx 0;
  padding-left: 160rpx;
}

.core {
  width: 260rpx;
  height: 260rpx;
  background-color: #FFD951;
  border-radius: 50%;
  box-shadow: 0px 0px 16px #FFD951;
  text-shadow: #000 6rpx 0 0, #000 0 6rpx 0, #000 -6rpx 0 0, #000 0 -6rpx 0;
  font-size: 50rpx;
  display: flex;
  align-items: center;
  flex-wrap: wrap;
  align-content: center;
  color: white;
  z-index: 10;
  animation: zoom 2s infinite;
  padding-left: 10rpx;
}

@keyframes zoom {
  0% {
    transform: scale(1);
  }

  50% {
    transform: scale(0.9);
  }

  100% {
    transform: scale(1);
  }
}
.cont2_imgBox image{
  width: 349rpx;
