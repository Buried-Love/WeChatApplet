  margin-top: -70rpx;
}
.core view {
  text-align: center;
  width: 100%;
  letter-spacing: 8rpx;
  font-weight: bold;
}

.branch {
  width: 130rpx;
  height: 130rpx;
  background-color: #FFD951;
  border-radius: 50%;
  position: absolute;
  text-align: center;
  letter-spacing: 8rpx;
  font-weight: bold;
  display: flex;
  align-items: center;
  justify-content: center;
  flex-wrap: wrap;
  align-content: center;
  color: white;
  z-index: 1;
  font-size: 38rpx;
  padding-left: 10rpx;
}

.branch::after {
  content: "";
  display: block;
  position: absolute;
  min-height: 150rpx;
  width: 3px;
  background-image: linear-gradient(to bottom, #EE9903 0%, #EE9903 60%, transparent 50%);
  background-size: 3px 15px;
  background-repeat: y-repeat;
  z-index: -1;
}

.branch1 {
  top: 10rpx;
  right: 200rpx;
  width: 120rpx;
  height: 120rpx;
}

.branch1::after {
  top: 100%;
  height: 90rpx;
  transform: rotate(-10deg) translateX(10rpx);
}

.branch2 {
  top: 50rpx;
  right: 0rpx;
}

.branch2::after {
  top: 50%;
  transform: rotate(35deg) translateX(10rpx) translateY(40rpx);
  left: 0rpx;
}

.branch3 {
  bottom: 40rpx;
  right: 0rpx;
  width: 140rpx;
  height: 140rpx;
}

.branch3::after {
  bottom: 100%;
  left: 0rpx;
  transform: rotate(-40deg) translateX(-40rpx) translateY(10rpx);
}

.branch4 {
  bottom: -180rpx;
  right: 80rpx;
  width: 160rpx;
  height: 160rpx;
}

.branch4::after {
  height: 330rpx;
  bottom: 100%;
  left: 0rpx;
  transform: rotate(-10deg) translateX(30rpx) translateY(10rpx);
}

.branch5 {
  bottom: 80rpx;
  left: 30rpx;
  width: 120rpx;
  height: 120rpx;
  flex-wrap: wrap;
}

.branch5::after {
  bottom: 100%;
  right: 0rpx;
  transform: rotate(60deg) translateX(90rpx) translateY(-20rpx);
}

.branch6 {
  top: 90rpx;
  left: 0rpx;
  width: 170rpx;
  height: 170rpx;
  flex-wrap: wrap;
}

.branch6::after {
  top: 100%;
  right: 0rpx;
  transform: rotate(-60deg) translateX(100rpx) translateY(10rpx);
}


