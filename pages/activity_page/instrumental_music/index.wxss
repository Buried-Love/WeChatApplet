.circular {
  width: 30rpx;
  height: 30rpx;
  border-radius: 50%;
  background-color: #f9f29d;
  text-align: center;
  line-height: 30rpx;
  font-size: 30rpx;
  color: #147023;
  display: inline-block;
  margin-right: 25rpx;
}

.title_block {
  width: 380rpx;
  height: 70rpx;
  background-color: #147023;
  margin: 0 auto;
  line-height: 70rpx;
  text-align: center;
  color: white;
  font-size: 30rpx;
  font-weight: bold;
  letter-spacing: 6rpx;
  border-radius: 50rpx;
  position: relative;
}

.title_block::after {
  content: "";
  display: block;
  width: 15rpx;
  height: 15rpx;
  background-color: white;
  border-radius: 50%;
  position: absolute;
  top: 28rpx;
  left: 50rpx;
}

.title_block::before {
  content: "";
  display: block;
  width: 15rpx;
  height: 15rpx;
  background-color: white;
  border-radius: 50%;
  position: absolute;
  top: 28rpx;
  right: 50rpx;
}

