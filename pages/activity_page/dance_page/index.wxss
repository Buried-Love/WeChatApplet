.offeringCourses {
  margin-top: 20rpx;
  position: relative;
  padding-top: 60rpx;
  padding-bottom: 40rpx;
}

.title_block {
  width: 380rpx;
  height: 70rpx;
  background-color: #5951D3;
  margin: 0 auto;
  line-height: 70rpx;
  text-align: center;
  color: white;
  font-size: 30rpx;
  font-weight: bold;
  letter-spacing: 6rpx;
  border-radius: 50rpx;
  position: relative;
}

.title_block::after {
  content: "";
  display: block;
  width: 15rpx;
  height: 15rpx;
  background-color: white;
  border-radius: 50%;
  position: absolute;
  top: 28rpx;
  left: 50rpx;
}

.title_block::before {
  content: "";
  display: block;
  width: 15rpx;
  height: 15rpx;
  background-color: white;
  border-radius: 50%;
  position: absolute;
  top: 28rpx;
  right: 50rpx;
}

.type_box {
  width: 590rpx;
  height: 600rpx;
  margin: 0 auto;
  display: flex;
  justify-content: center;
  align-items: center;
  position: relative;
}

.core {
  width: 200rpx;
  height: 200rpx;
  background-color: white;
  border-radius: 50%;
  box-shadow: 0px 0px 16px #7F7DDC;
  font-size: 50rpx;
  display: flex;
  align-items: center;
  flex-wrap: wrap;
  align-content: center;
  z-index: 10;
  animation: zoom 2s infinite;
}

@keyframes zoom {
  0% {
    transform: scale(1);
  }

  50% {
    transform: scale(0.9);
  }

  100% {
    transform: scale(1);
  }
}

.core view {
  text-align: center;
  width: 100%;
  letter-spacing: 8rpx;
  font-weight: bold;
}

.branch {
  width: 130rpx;
  height: 130rpx;
  background-color: #7F7DDC;
  border-radius: 50%;
  position: absolute;
  text-align: center;
  letter-spacing: 8rpx;
  font-weight: bold;
  display: flex;
  align-items: center;
  justify-content: center;
  flex-wrap: wrap;
  align-content: center;
  color: white;
  z-index: 1;
  font-size: 32rpx;
}

.branch::after {
  content: "";
  display: block;
  position: absolute;
  min-height: 150rpx;
  width: 3px;
  background-image: linear-gradient(to bottom, #4340C3 0%, #4340C3 80%, transparent 50%);
  background-size: 3px 15px;
  background-repeat: y-repeat;
  z-index: -1;
}

.branch1 {
  top: 20rpx;
  left: 230rpx;
  width: 150rpx;
  height: 150rpx;
}

.branch1::after {
  top: 100%;
  height: 90rpx;
  transform: rotate(5deg) translateX(-10rpx);
}

.branch2 {
  top: 130rpx;
  right: 20rpx;
}

.branch2::after {
  top: 50%;
  transform: rotate(60deg) translateX(-40rpx) translateY(50rpx);
  left: 0rpx;
}

.branch3 {
  bottom: 80rpx;
  right: 40rpx;
  width: 140rpx;
  height: 140rpx;
}

.branch3::after {
  bottom: 100%;
  left: 0rpx;
  transform: rotate(-50deg) translateX(-70rpx) translateY(0rpx);
}

.branch4 {
  bottom: 10rpx;
  right: 250rpx;
  width: 110rpx;
  height: 110rpx;
}

.branch4::after {
  bottom: 100%;
  left: 0rpx;
  transform: rotate(-1deg) translateX(50rpx);
}

.branch5 {
  bottom: 70rpx;
  left: 0rpx;
  width: 170rpx;
  height: 170rpx;
  flex-wrap: wrap;
}
.branch5::after {
  bottom: 100%;
  right: 0rpx;
  transform: rotate(50deg) translateX(80rpx) translateY(6rpx);
}
.branch6 {
  top: 90rpx;
  left: 40rpx;
  width: 120rpx;
  height: 120rpx;
  flex-wrap: wrap;
}
.branch6::after {
  top: 100%;
  right: 0rpx;
  transform: rotate(-48deg) translateX(55rpx);
}
